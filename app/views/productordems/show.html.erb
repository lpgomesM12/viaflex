<div class="page-header">
  <%= link_to productordems_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_productordem_path(@productordem), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show productordem</h1>
</div>

<dl class="dl-horizontal">
  <dt>Serviceordem:</dt>
  <dd><%= @productordem.serviceordem %></dd>

  <dt>Product:</dt>
  <dd><%= @productordem.product %></dd>

  <dt>Valor product:</dt>
  <dd><%= @productordem.valor_product %></dd>

  <dt>Qtd product:</dt>
  <dd><%= @productordem.qtd_product %></dd>

</dl>
